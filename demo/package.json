    "coffee": "^3.3.3",
    "coffee-loader": "0.9.0",
    "coffeescript": "^1.12.7",
    "coffeescript-ui": "../",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "html-loader": "^0.5.1",
    "less": "^2.7.2",
