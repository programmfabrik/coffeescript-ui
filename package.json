{
  "name": "coffeescript-ui",
  "version": "0.9.0",
  "description": "Coffeescript User Interface System",
  "scripts": {
    "build": "webpack --progress",
    "build:watch": "webpack --watch --progress",
    "watch:nocss": "webpack --watch --progress --env.noCss",
    "build:nocss": "webpack --progress --env.noCss",
    "build:all": "npm run build && npm run build -- --env.noCss && npm run build -- --env.minify && npm run build -- --env.minify --env.noCss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programmfabrik/coffeescript-ui.git"
  },
  "main": "public/cui.min.js",
  "keywords": [
    "coffeescript",
    "ui",
    "user",
    "interface",
    "gui",
    "interface",
    "library"
  ],
  "author": "Programmfabrik GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/programmfabrik/coffeescript-ui/issues"
  },
  "homepage": "https://github.com/programmfabrik/coffeescript-ui#readme",
  "devDependencies": {
    "base64-inline-loader": "^1.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "coffee": "^3.3.0",
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.7",
    "css-loader": "^0.28.4",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "html-loader": "^0.5.1",
    "less": "^2.7.2",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "sass": "^0.5.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.0"
  }
}
